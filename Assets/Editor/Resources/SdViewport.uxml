<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="True">
    <uie:Toolbar>
        <uie:ToolbarButton text="Save" display-tooltip-when-elided="true" name="save_btn" style="width: 50px; -unity-text-align: middle-center;" />
        <uie:ToolbarButton text="Clear" display-tooltip-when-elided="true" name="clear_btn" style="width: 50px; -unity-text-align: middle-center;" />
        <uie:ToolbarSpacer style="flex-grow: 1;" />
    </uie:Toolbar>
    <ui:ScrollView style="flex-grow: 1;">
        <ui:VisualElement name="image_layer" style="display: flex; padding-left: 50%; padding-right: 50%; padding-top: 50%; padding-bottom: 50%; width: 0; height: 0; position: absolute;" />
        <ui:VisualElement name="mask_layer" text="clear" style="padding-left: 49%; padding-right: 49%; padding-top: 49%; padding-bottom: 49%; width: 0; height: 0; margin-left: 1%; margin-right: 1%; margin-top: 1%; margin-bottom: 1%; position: absolute; display: none; visibility: hidden; overflow: hidden;" />
    </ui:ScrollView>
</ui:UXML>
